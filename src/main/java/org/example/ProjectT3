int[][] num = {
                {3, 4, 5, 1, 6, 7},
                {83, 44, 5, 3, 7, 4},
                {3, 6, 2, 34, 55, 70},
                {5, 6, 2, 11, 7, 9}
        };
        for (int i = 0; i < num.length; i++) {
            for (int j = 0; j < num[i].length; j++) {
                if (num[i][j] % 2 == 0) {
                    System.out.print(num[i][j] + " ");
                }

            }
            System.out.println();
        }
    }
}
